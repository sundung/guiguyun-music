page {
  height: 100%;
}
.personalFMContainer {
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  display: flex;
  flex-direction: column;
  align-items: center;
}
.personalFMContainer .content .musicImg {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  width: 400rpx;
  height: 400rpx;
}
.personalFMContainer .content .comm {
  margin-top: 150rpx;
  text-align: center;
  color: #fff;
}
.personalFMContainer .content .comm .artistsName {
  margin-top: 20rpx;
  font-size: 22rpx;
}
.personalFMContainer .musicControl {
  position: absolute;
  bottom: 40rpx;
  left: 0;
  border-top: 1rpx solid #fff;
  width: 100%;
  display: flex;
}
.personalFMContainer text {
  width: 20%;
  height: 120rpx;
  line-height: 120rpx;
  text-align: center;
  color: #fff;
  font-size: 50rpx;
}
.personalFMContainer .big {
  font-size: 80rpx;
}
