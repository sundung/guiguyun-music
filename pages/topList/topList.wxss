.topListContainer {
  padding: 20rpx;
}
.topListContainer .topListContent {
  display: flex;
  padding: 20rpx;
  box-sizing: border-box;
  height: 200rpx;
  background-color: #eee;
  margin: 10rpx 0;
  border-radius: 10rpx;
  position: relative;
}
.topListContainer .topListContent .updateFrequency {
  position: absolute;
  top: 10rpx;
  right: 20rpx;
  color: #666;
  font-size: 22rpx;
}
.topListContainer .topListContent .left {
  width: 30%;
}
.topListContainer .topListContent .left .title {
  font-size: 32rpx;
  color: red;
  width: 150rpx;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.topListContainer .topListContent .left image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 10rpx;
}
.topListContainer .topListContent .right .musicItem {
  margin-top: 10rpx;
  font-size: 26rpx;
  display: flex;
  width: 350rpx;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.topListContainer .topListContent .right .musicItem .index {
  width: 40rpx;
  text-align: center;
}
.topListContainer .songType .title {
  padding: 20rpx;
}
.topListContainer .songType .songTypeContent {
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  flex-wrap: wrap;
}
.topListContainer .songType .songTypeContent .item {
  width: 30%;
  text-align: center;
  margin-top: 10rpx;
  position: relative;
}
.topListContainer .songType .songTypeContent .item .time {
  position: absolute;
  top: 10rpx;
  right: 15rpx;
  color: #fff;
  border: 1px solid #fff;
  padding: 0 10rpx;
  border-radius: 30rpx;
}
.topListContainer .songType .songTypeContent .item image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
}
.topListContainer .songType .songTypeContent .item view {
  font-size: 22rpx;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
